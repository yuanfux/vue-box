{
  "name": "vue-box",
  "version": "1.0.0",
  "main": "./dist/vue-box.common.js",
  "files": [
    "dist/vue-box.common.js",
    "dist/vue-box.umd.js",
    "dist/vue-box.umd.min.js",
    "src/components/Box.vue",
    "README.md"
  ],
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-doc": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name vue-box ./src/components/Box.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
